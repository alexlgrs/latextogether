{
  "commands": [
    { "label": "\\documentclass", "insert": "documentclass[${1:12pt}]{${2:article}}", "kind": "Snippet", "desc": "Type de document" },
    { "label": "\\usepackage", "insert": "usepackage{${1:package}}", "kind": "Snippet", "desc": "Importer un package" },
    { "label": "\\maketitle", "insert": "maketitle", "kind": "Snippet", "desc": "Générer le titre" },
    { "label": "\\tableofcontents", "insert": "tableofcontents", "kind": "Snippet", "desc": "Table des matières" },
    { "label": "\\section", "insert": "section{${1:titre}}", "kind": "Snippet", "desc": "Titre de section" },
    { "label": "\\subsection", "insert": "subsection{${1:titre}}", "kind": "Snippet", "desc": "Sous-section" },
    { "label": "\\subsubsection", "insert": "subsubsection{${1:titre}}", "kind": "Snippet", "desc": "Sous-sous-section" },
    { "label": "\\textbf", "insert": "textbf{${1:texte}}", "kind": "Snippet", "desc": "Gras" },
    { "label": "\\textit", "insert": "textit{${1:texte}}", "kind": "Snippet", "desc": "Italique" },
    { "label": "\\underline", "insert": "underline{${1:texte}}", "kind": "Snippet", "desc": "Souligné" },
    { "label": "\\texttt", "insert": "texttt{${1:texte}}", "kind": "Snippet", "desc": "Police machine à écrire" },

    { "label": "\\begin{align}", "insert": "begin{align}\n\t${1:e} &= ${2:mc^2}\n\\end{align}", "kind": "Snippet", "desc": "Équations alignées" },
    { "label": "\\begin{itemize}", "insert": "begin{itemize}\n\t\\item ${1}\n\\end{itemize}", "kind": "Snippet", "desc": "Liste à puces" },
    { "label": "\\begin{enumerate}", "insert": "begin{enumerate}\n\t\\item ${1}\n\\end{enumerate}", "kind": "Snippet", "desc": "Liste numérotée" },
    { "label": "\\begin{matrix}", "insert": "begin{matrix} ${1:a} & ${2:b} \\\\ ${3:c} & ${4:d} \\end{matrix}", "kind": "Snippet", "desc": "Matrice simple" },
    { "label": "\\begin{pmatrix}", "insert": "begin{pmatrix} ${1:a} & ${2:b} \\\\ ${3:c} & ${4:d} \\end{pmatrix}", "kind": "Snippet", "desc": "Matrice avec parenthèses" },
    { "label": "\\begin{bmatrix}", "insert": "begin{bmatrix} ${1:a} & ${2:b} \\\\ ${3:c} & ${4:d} \\end{bmatrix}", "kind": "Snippet", "desc": "Matrice avec crochets" },
    { "label": "\\begin{cases}", "insert": "begin{cases} ${1:x} & \\text{si } ${2:condition} \\\\ ${3:y} & \\text{sinon} \\end{cases}", "kind": "Snippet", "desc": "Système / Cas" },
    { "label": "\\begin{figure}", "insert": "begin{figure}[h]\n\t\\centering\n\t\\includegraphics[width=0.8\\textwidth]{${1:image.png}}\n\t\\caption{${2:Légende}}\n\\end{figure}", "kind": "Snippet", "desc": "Inclusion d'image" },

    { "label": "\\frac", "insert": "frac{${1:num}}{${2:den}}", "kind": "Snippet", "desc": "Fraction" },
    { "label": "\\sqrt", "insert": "sqrt{${1:x}}", "kind": "Snippet", "desc": "Racine carrée" },
    { "label": "\\sqrt[n]", "insert": "sqrt[${1:n}]{${2:x}}", "kind": "Snippet", "desc": "Racine n-ième" },
    { "label": "\\sum", "insert": "sum_{${1:i=0}}^{${2:n}} $0", "kind": "Snippet", "desc": "Somme" },
    { "label": "\\int", "insert": "int_{${1:a}}^{${2:b}} $0", "kind": "Snippet", "desc": "Intégrale" },
    { "label": "\\prod", "insert": "prod_{${1:i=1}}^{${2:n}} $0", "kind": "Snippet", "desc": "Produit" },
    { "label": "\\lim", "insert": "lim_{${1:x \\to \\infty}} $0", "kind": "Snippet", "desc": "Limite" },

    { "label": "\\sin", "insert": "sin(${1:x})", "kind": "Function", "desc": "Sinus" },
    { "label": "\\cos", "insert": "cos(${1:x})", "kind": "Function", "desc": "Cosinus" },
    { "label": "\\tan", "insert": "tan(${1:x})", "kind": "Function", "desc": "Tangente" },
    { "label": "\\exp", "insert": "exp(${1:x})", "kind": "Function", "desc": "Exponentielle" },
    { "label": "\\ln", "insert": "ln(${1:x})", "kind": "Function", "desc": "Logarithme népérien" },
    { "label": "\\log", "insert": "log_{${1:10}}(${2:x})", "kind": "Function", "desc": "Logarithme" },

    { "label": "\\alpha", "insert": "alpha", "kind": "Variable", "desc": "α" },
    { "label": "\\beta", "insert": "beta", "kind": "Variable", "desc": "β" },
    { "label": "\\gamma", "insert": "gamma", "kind": "Variable", "desc": "γ" },
    { "label": "\\delta", "insert": "delta", "kind": "Variable", "desc": "δ" },
    { "label": "\\epsilon", "insert": "epsilon", "kind": "Variable", "desc": "ε" },
    { "label": "\\zeta", "insert": "zeta", "kind": "Variable", "desc": "ζ" },
    { "label": "\\eta", "insert": "eta", "kind": "Variable", "desc": "η" },
    { "label": "\\theta", "insert": "theta", "kind": "Variable", "desc": "θ" },
    { "label": "\\iota", "insert": "iota", "kind": "Variable", "desc": "ι" },
    { "label": "\\kappa", "insert": "kappa", "kind": "Variable", "desc": "κ" },
    { "label": "\\lambda", "insert": "lambda", "kind": "Variable", "desc": "λ" },
    { "label": "\\mu", "insert": "mu", "kind": "Variable", "desc": "μ" },
    { "label": "\\nu", "insert": "nu", "kind": "Variable", "desc": "ν" },
    { "label": "\\xi", "insert": "xi", "kind": "Variable", "desc": "ξ" },
    { "label": "\\pi", "insert": "pi", "kind": "Variable", "desc": "π" },
    { "label": "\\rho", "insert": "rho", "kind": "Variable", "desc": "ρ" },
    { "label": "\\sigma", "insert": "sigma", "kind": "Variable", "desc": "σ" },
    { "label": "\\tau", "insert": "tau", "kind": "Variable", "desc": "τ" },
    { "label": "\\phi", "insert": "phi", "kind": "Variable", "desc": "φ" },
    { "label": "\\chi", "insert": "chi", "kind": "Variable", "desc": "χ" },
    { "label": "\\psi", "insert": "psi", "kind": "Variable", "desc": "ψ" },
    { "label": "\\omega", "insert": "omega", "kind": "Variable", "desc": "ω" },
    { "label": "\\Gamma", "insert": "Gamma", "kind": "Variable", "desc": "Γ" },
    { "label": "\\Delta", "insert": "Delta", "kind": "Variable", "desc": "Δ" },
    { "label": "\\Theta", "insert": "Theta", "kind": "Variable", "desc": "Θ" },
    { "label": "\\Lambda", "insert": "Lambda", "kind": "Variable", "desc": "Λ" },
    { "label": "\\Pi", "insert": "Pi", "kind": "Variable", "desc": "Π" },
    { "label": "\\Sigma", "insert": "Sigma", "kind": "Variable", "desc": "Σ" },
    { "label": "\\Phi", "insert": "Phi", "kind": "Variable", "desc": "Φ" },
    { "label": "\\Psi", "insert": "Psi", "kind": "Variable", "desc": "Ψ" },
    { "label": "\\Omega", "insert": "Omega", "kind": "Variable", "desc": "Ω" },

    { "label": "\\infty", "insert": "infty", "kind": "Value", "desc": "∞" },
    { "label": "\\times", "insert": "times", "kind": "Operator", "desc": "×" },
    { "label": "\\cdot", "insert": "cdot", "kind": "Operator", "desc": "Point multiplié" },
    { "label": "\\pm", "insert": "pm", "kind": "Operator", "desc": "±" },
    { "label": "\\approx", "insert": "approx", "kind": "Operator", "desc": "≈" },
    { "label": "\\neq", "insert": "neq", "kind": "Operator", "desc": "≠" },
    { "label": "\\le", "insert": "le", "kind": "Operator", "desc": "≤" },
    { "label": "\\ge", "insert": "ge", "kind": "Operator", "desc": "≥" },
    { "label": "\\ll", "insert": "ll", "kind": "Operator", "desc": "Très inférieur à" },
    { "label": "\\gg", "insert": "gg", "kind": "Operator", "desc": "Très supérieur à" },

    { "label": "\\leftarrow", "insert": "leftarrow", "kind": "Operator", "desc": "←" },
    { "label": "\\rightarrow", "insert": "rightarrow", "kind": "Operator", "desc": "→" },
    { "label": "\\uparrow", "insert": "uparrow", "kind": "Operator", "desc": "↑" },
    { "label": "\\downarrow", "insert": "downarrow", "kind": "Operator", "desc": "↓" },
    { "label": "\\Rightarrow", "insert": "Rightarrow", "kind": "Operator", "desc": "⇒" },
    { "label": "\\Leftrightarrow", "insert": "Leftrightarrow", "kind": "Operator", "desc": "⇔" },
    { "label": "\\mapsto", "insert": "mapsto", "kind": "Operator", "desc": "↦" },

    { "label": "\\forall", "insert": "forall", "kind": "Value", "desc": "∀" },
    { "label": "\\exists", "insert": "exists", "kind": "Value", "desc": "∃" },
    { "label": "\\in", "insert": "in", "kind": "Operator", "desc": "∈" },
    { "label": "\\notin", "insert": "notin", "kind": "Operator", "desc": "∉" },
    { "label": "\\subset", "insert": "subset", "kind": "Operator", "desc": "⊂" },
    { "label": "\\subseteq", "insert": "subseteq", "kind": "Operator", "desc": "⊆" },
    { "label": "\\cap", "insert": "cap", "kind": "Operator", "desc": "Intersection" },
    { "label": "\\cup", "insert": "cup", "kind": "Operator", "desc": "Union" },
    { "label": "\\emptyset", "insert": "emptyset", "kind": "Value", "desc": "Ø" },
    { "label": "\\nabla", "insert": "nabla", "kind": "Value", "desc": "Opérateur Nabla" },
    { "label": "\\partial", "insert": "partial", "kind": "Value", "desc": "Dérivée partielle" },

    { "label": "\\bar", "insert": "bar{${1:x}}", "kind": "Snippet", "desc": "Barre au dessus" },
    { "label": "\\hat", "insert": "hat{${1:x}}", "kind": "Snippet", "desc": "Chapeau" },
    { "label": "\\tilde", "insert": "tilde{${1:x}}", "kind": "Snippet", "desc": "Tilde" },
    { "label": "\\vec", "insert": "vec{${1:v}}", "kind": "Snippet", "desc": "Vecteur" },
    { "label": "\\dot", "insert": "dot{${1:x}}", "kind": "Snippet", "desc": "Dérivée temporelle (point)" },

    { "label": "\\left(", "insert": "left( ${1:x} \\right)", "kind": "Snippet", "desc": "Parenthèses extensibles" },
    { "label": "\\left[", "insert": "left[ ${1:x} \\right]", "kind": "Snippet", "desc": "Crochets extensibles" },
    { "label": "\\left\\{", "insert": "left\\{ ${1:x} \\right\\}", "kind": "Snippet", "desc": "Accolades extensibles" },

    { "label": "\\quad", "insert": "quad", "kind": "Value", "desc": "Espace moyen" },
    { "label": "\\qquad", "insert": "qquad", "kind": "Value", "desc": "Grand espace" },
    { "label": "\\text{}", "insert": "text{${1:texte}}", "kind": "Snippet", "desc": "Texte normal en mode math" }
  ]
}